{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-config-prettier"
  ],
  "rulesDirectory": [
    "tslint-plugin-prettier"
  ],
  "jsRules": true,
  "rules": {
    "prettier": [true, ".prettierrc"],
    "object-literal-sort-keys": false
  },
  "linterOptions": {
    "exclude": [
      "node_modules/**"
    ]
  }
}
